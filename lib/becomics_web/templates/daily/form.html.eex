<%= form_for @conn, comic_html_path(@conn, :update, @comic.id), fn f -> %>
  <%= render "error.html", f: f %>

  <div class="form-group">
    <label><%= @comic.name %></label>
    <%= text_input f, :url, class: "form-control", autocomplete: "off" %>
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
